program id { 

function int factorial(int n) {
  int i = 1;
  int acum = 1;
  while(i < n){
    acum = acum*i;
    i = i+1;

   }
  return acum;
}


  priomh {
    int x=1, n=0;
    print("Read N");
    read (n);
    print("-----------------------");

    print("Iterative Factorial");
    x=factorial(n);
    print(x);

   
  }
}